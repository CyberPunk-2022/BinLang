<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xianglan.qnytv.mapper.VideoMapper">

    <select id="selectVideosByUserId" parameterType="java.lang.Long" resultType="com.xianglan.qnytv.domain.VideoPo">
        select
            *
        from
            t_video
        where
            userId = #{userId}
    </select>



</mapper>